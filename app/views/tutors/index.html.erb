<p id="notice"><%= notice %></p>

<h1>Tutors</h1>

<table>
  <thead>
    <tr>
      <th>Rut</th>
      <th>Nombre</th>
      <th>Apellido pat</th>
      <th>Apellido mat</th>
      <th>Direccion</th>
      <th>Cargo</th>
      <th>Sueldo hora</th>
      <th>Comuna</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tutors.each do |tutor| %>
      <tr>
        <td><%= tutor.rut %></td>
        <td><%= tutor.nombre %></td>
        <td><%= tutor.apellido_pat %></td>
        <td><%= tutor.apellido_mat %></td>
        <td><%= tutor.direccion %></td>
        <td><%= tutor.cargo %></td>
        <td><%= tutor.sueldo_hora %></td>
        <td><%= tutor.comuna_id %></td>
        <td><%= link_to 'Show', tutor %></td>
        <td><%= link_to 'Edit', edit_tutor_path(tutor) %></td>
        <td><%= link_to 'Destroy', tutor, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tutor', new_tutor_path %>
